{
	"compilerOptions": {
		"target": "es2015",
		"module": "es2015",
		"moduleResolution": "node",
		"jsx": "react",
		"reactNamespace": "app",
		"lib": ["dom", "esnext"],
		"experimentalDecorators": true,
		"sourceMap": true,
		"esModuleInterop": true,
		"downlevelIteration": true,
		"allowJs": true,
		"outDir": "coverage/src",
		"moduleResolution": "node"
	}
}
